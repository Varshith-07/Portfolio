<%- layout('/layout/boilerplate.ejs') %>
<style>
.accordion{
    background-color: rgb(69, 69, 69);
    color: white;
    margin: auto;
    width: 80%;
    border: 3px solid rgb(0, 0, 0);
    padding: 10px;
    margin-top: 4rem;
    max-width: 1080px;
}
.accordion-body {
